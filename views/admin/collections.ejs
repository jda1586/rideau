<!DOCTYPE html>
<html class="no-js" lang="en" dir="ltr">
<head>
    <meta charset="UTF-8"/>
    <meta http-equiv="x-ua-compatible" content="ie=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Rideau</title>
    <link href="/stylesheets/font-awesome.min.css" rel="stylesheet"/>
    <link href="/stylesheets/foundation.min.css" rel="stylesheet"/>
    <link href="/stylesheets/basic.css" rel="stylesheet"/>
    <script src="/javascripts/jquery-2.2.4.min.js"></script>
</head>

<body>
<% include _header.ejs %>

<div class="row">
    <div class="large-12 columns">
        <dl class="sub-nav margin-top-25">
            <dt>Filter:</dt>
            <dd class="active"><a href="#">All</a></dd>
        </dl>
    </div>
</div>

<div class="row">
    <div class="large-12 columns">
        <table width="100%">
            <thead>
            <tr>
                <th>Name</th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            <%
            Object.keys(collections).forEach(function (name) {
                data = collections[name];
            %>
            <tr>
                <td>
                    <%- data.name %>
                </td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <%
            })
            %>
            </tbody>
        </table>
    </div>
</div>

<script src="/javascripts/foundation/foundation.js" type="text/javascript"></script>
<script src="/javascripts/foundation/foundation.dropdown.js" type="text/javascript"></script>

<script>
    $(document).ready(function () {

    });
    $(document).foundation();
</script>
</body>
</html>